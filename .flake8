[flake8]
ignore =
    E501,
    W503,
    E203,
    E266,
    E402,
    F401,
    F403,
    F405,
    E722,
    E731,
    B007,
    B006,
max-line-length = 120
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .mypy_cache,
    .pytest_cache,
    build,
    dist
per-file-ignores =
    __init__.py:F401